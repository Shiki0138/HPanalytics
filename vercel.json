{
  "version": 2,
  "public": true,
  "builds": [],
  "rewrites": [
    {
      "source": "/ec",
      "destination": "/public/simple-analytics.html"
    },
    {
      "source": "/corporate",
      "destination": "/public/corporate-analytics.html"
    },
    {
      "source": "/dashboard",
      "destination": "/public/production-dashboard.html"
    },
    {
      "source": "/install",
      "destination": "/public/production-install.html"
    },
    {
      "source": "/tracking-tag.js",
      "destination": "/public/tracking-tag-production.js"
    },
    {
      "source": "/analytics-viewer",
      "destination": "/public/analytics-viewer.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        }
      ]
    },
    {
      "source": "/tracking-tag.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}